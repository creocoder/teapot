{
  "name": "shrikeh/php-default-template",
  "description": "Replace this",
  "license": "MIT",
  "authors": [
    {
      "name": "Barney Hanlon",
      "email": "barney@shrikeh.net"
    }
  ],
  "require-dev": {
    "phpspec/phpspec": "~3.0.0",
    "bossa/phpspec2-expect": "^2.0",
    "behat/behat": "^3.2",
    "squizlabs/php_codesniffer": "^2.7",
    "escapestudios/symfony2-coding-standard": "^2.9",
    "phpmd/phpmd": "^2.5"
  },
  "config": {
    "bin-dir": "bin"
  },
  "archive": {
    "exclude": ["*", "!/src"]
  },
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Teapot\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "examples\\Teapot\\": "examples"
    }
  }
}
